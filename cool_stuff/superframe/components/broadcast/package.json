{
  "name": "aframe-broadcast-component",
  "version": "0.1.1",
  "description": "Multi-user in A-Frame using raw websockets",
  "main": "index.js",
  "scripts": {
    "build": "browserify examples/main.js -o examples/build.js",
    "dev": "budo examples/main.js:examples/build.js --port 8000 --live --open",
    "dist": "webpack index.js dist/aframe-broadcast-component.js && webpack -p index.js dist/aframe-broadcast-component.min.js",
    "postpublish": "npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supermedium/superframe.git"
  },
  "keywords": [
    "aframe",
    "aframe-component",
    "aframe-vr",
    "vr",
    "mozvr",
    "webvr"
  ],
  "author": "Kevin Ngo <me@ngokevin.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/supermedium/superframe/issues"
  },
  "homepage": "https://github.com/supermedium/superframe/tree/components/broadcast#readme",
  "devDependencies": {
    "aframe": "^0.9.2",
    "browserify": "^12.0.1",
    "budo": "^7.1.0",
    "shelljs": "^0.6.0",
    "webpack": "^1.12.9"
  },
  "dependencies": {
    "socket.io": "^1.4.6",
    "socket.io-client": "^1.4.6"
  }
}
