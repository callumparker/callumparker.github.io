  "bugs": {
    "url": "https://github.com/supermedium/superframe/issues"
  },
  "homepage": "https://github.com/supermedium/superframe/tree/look-at#readme",
  "devDependencies": {
    "aframe": "^0.5.0",
    "browserify": "^12.0.1",
    "budo": "^7.1.0",
    "shelljs": "^0.6.0",
    "webpack": "^1.12.9"
  }
